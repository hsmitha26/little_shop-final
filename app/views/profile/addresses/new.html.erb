<%= form_for @address do |f| %>
  <p>
    <%= f.label :nickname %>:
    <%= f.text_field :nickname, required: true %>
  </p>

  <p>
    <%= f.label :street %>:
    <%= f.text_field :street, required: true %>
  </p>

  <p>
    <%= f.label :city %>:
    <%= f.text_field :city, required: true %>
  </p>

  <p>
    <%= f.label :state %>:
    <%= f.text_field :state, required: true %>
  </p>

  <p>
    <%= f.label :zip %>:
    <%= f.text_field :zip, required: true %>
  </p>
<% end %>
